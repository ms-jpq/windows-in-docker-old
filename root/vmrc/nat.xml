<network>
  <name>${NAT_NAME}</name>
  <bridge name="${NAT_NAME}"/>
  <forward mode="nat"/>
  <mac address="${BR_MAC}"/>
  <ip address="${ROUTER}" netmask="${MASK}">
    <dhcp>
      <range start="${BEGIN}" end="${END}"/>
      <host mac="${VM_MAC}" name="${VM_NAME}" ip="${VM_IP}"/>
    </dhcp>
  </ip>
</network>

